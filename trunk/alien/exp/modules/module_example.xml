<?xml version="1.0" encoding="windows-1251" ?> 
<module>
  <name>Example module</name> 
  <unixname>example_module</unixname>
  <version>0.0.1</version>
  <status>beta</status>
  <compatible>
  	<system>
  		<alien>
			<version>1.0</version>
			<externalModules>
				<externalModule>
					<name>alien_news</name>
					<version>1.0</version>
					<download>http://never-invited.com/alien/modules/alien_news/alien_news-1.0.tar.bz2</download>
					<update>Yes</update>
					<laterVersions>Yes</laterVersions>
					<earlierVersions>No</earlierVersions>
				</externalModule>
			</externalModules>
		</alien>
		<PHP>
			<version>5.0.0</version>
			<laterVersions>Yes</laterVersions>
			<earlierVersions>No</earlierVersions>
			<extensions>
				<extension>
					<name>php_bz2</name>
					<tryToLoad>Yes</tryToLoad>
					<necessarily>Yes</necessarily>
				</extension>
			</extensions>
			<testedWith>
				<versions>
					<version>5.0.0</version>
					<version>5.0.1</version>
					<version>5.0.2</version>
					<version>5.0.3</version>
					<version>5.0.4</version>
					<version>5.1.0</version>
				</versions>
				<ifNotTested>
					<warn>Yes</warn>
					<stop>No</stop>
				</ifNotTested>
			</testedWith>
		</PHP>
		<webservers>
			<webserver>
				<name>Apache</name>
				<versions>
					<version>1.3.x</version>
					<version>2.x.x</version>
				</versions>
				<testedWith>
					<!-- Tested Versions !-->
				</testedWith>
				<ifNotTested>
					<warn>No</warn>
					<stop>No</stop>
				</ifNotTested>
			</webserver>
			<ifNotTested>
				<warn>No</warn>
				<stop>Yes</stop>
			</ifNotTested>
		</webservers>
		<os>
			<!-- Can be just empty -->
		</os>
		<dbs>
			<db>
				<name>MySQL</name>
				<versions>
					<version>3.2.x</version>
					<version>4.0.x</version>
					<version>4.1.x</version>
				</versions>
				<accessWrapper>
					<name>alien_mysql</name>
					<version>1.0.0</version>
					<download>http://never-invited.com/alien/modules/alien_mysql/alien_mysql-1.0.tar.bz2</download>
					<update>Yes</update>
					<laterVersions>Yes</laterVersions>
					<earlierVersions>No</earlierVersions>
				</accessWrapper>
			</db>
			<ifNotTested>
				<warn>No</warn>
				<stop>Yes</stop>
			</ifNotTested>
		</dbs>
		<check>
			<checkFile>myCheck.php</checkFile>
			<checkFunction>checkEnv()</checkFunction>
		</check>
	</system>
  </compatible>
  <templates>
  <template>
  		<action>view</action>
  		<xmlname>module_view.xml</xmlname>
  		<xsltname>module_view.xslt</xsltname>
  </template>
  </templates>
</module>